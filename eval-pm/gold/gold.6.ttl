:this a eval:Node .
:favor a eval:Node .
:our a eval:Node .
:syrian a eval:Node .
:brothers a eval:Node .
:us a eval:Node .
:iraq_1 a eval:Node .
:veteran a eval:Node .
:qaeda a eval:Node .
:iraq_2 a eval:Node .
:who a eval:Node .
:he a eval:Node .
:front a eval:Node .
